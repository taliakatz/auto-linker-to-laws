{
    "name": "Israeli laws auto-linker",
    "description": "Automatically links israeli laws references to the Knesset website",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.js",
                "main.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/laws_icon16.png",
        "48": "icons/laws_icon48.png",
        "128": "icons/laws_icon128.png"
    }
}