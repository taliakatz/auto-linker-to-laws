{
    "name": "Israeli laws auto-linker",
    "description": "Automatically links israeli laws references to the Knesset website",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.js",
                "findAndReplace.js"
            ],
            "css": [
                "theme.css"
            ]
        }
    ],
    "page_action": {
        "default_icon": {
          "1": "laws_icon.png"
        }
      },
      "icons": {
        "1": "laws_icon.png"
      }
}